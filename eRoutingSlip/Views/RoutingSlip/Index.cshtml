@model IEnumerable<eRoutingSlip.Models.RoutingSlip>

@{
    ViewBag.Title = "Index";
}
<div class="container mt-3">
    <h3>Active Routing Slips</h3>

    <table class="table">
        <tr>
            <th>
                Division
            </th>
            <th>
                Document
            </th>

            <th>
                Date
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DivisionID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocumentName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.DateSubmitted)
            </td>


            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.RoutingSlipID }) |
                @Html.ActionLink("Details", "Details", new { id = item.RoutingSlipID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.RoutingSlipID })
            </td>
        </tr>
        }

    </table>
</div>